  <node name="robot_state_publisher" pkg="robot_state_publisher" type="state_publisher"/>
  
  <group ns="hardware">
    <!-- <node name="kinect_man" pkg="kinect_man" type="kinect_man_node" output="screen"/> -->
  </group>

  <group ns="hri">
